    "client": "cd assets && npx honeypack start --host 0.0.0.0",
    "eslint": "eslint --fix --ext .js --ext .jsx --ignore-path .eslintignore ."
  },
  "license": "MIT",
  "dependencies": {
    "async": "3.2.0",
    "dmdb": "1.0.19862",
    "editor-json-parser": "1.1.1",
    "formstream": "1.1.1",
    "hc-bee": "^1.0.21",
    "hc-signature-auth": "1.0.7",
    "ip": "1.1.4",
    "lodash": "4.17.21",
    "moment": "2.19.3",
    "multer": "^1.4.5-lts.1",
    "mysql": "2.18.1",
    "pg": "8.11.3",
    "queue": "^6.0.2",
    "svg-captcha": "^1.4.0",
    "tar": "^6.1.11",
    "urllib": "2.38.0",
    "uuid": "^8.1.0",
    "xfs": "^0.2.4",
    "yaml": "^1.10.0"
  },
  "devDependencies": {
    "sql.js": "^1.1.0",
    "http-proxy-middleware": "^1.0.4",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.2.0",
    "babel-eslint": "^8.2.3",
    "concurrently": "^5.2.0",
    "eslint": "^6.0.0",
    "eslint-config-ali": "^9.0.2",
    "eslint-config-aliyun": "^2.1.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "husky": "=4.2.5",
    "install": "^0.13.0",
    "mm": "2.1.0",
    "mocha": "3.2.0",
    "npm-run-all": "=4.1.5",
    "should": "11.1.2",
    "supertest": "3.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run eslint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
